# OpenAI API Key for landscape description generation
OPENAI_API_KEY=your_openai_api_key_here

# Replicate API Token for FLUX.1 Fill image generation
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Image generation method: 'flux' or 'sdxl' (optional, defaults to 'flux')
IMAGE_GENERATION_METHOD=flux

# Base URL for the application (optional, defaults to http://localhost:3000)
BASE_URL=http://localhost:3000

# Path to Python executable (optional, uses system default if not set)
PYTHON_PATH=python3
